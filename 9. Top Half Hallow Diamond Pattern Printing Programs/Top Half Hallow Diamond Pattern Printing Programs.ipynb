   "source": [
    "## To print Top Half Hallow Diamond Pattern with * symbols"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "dc1edf2a",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter the number of rows5\n",
      "        * \n",
      "      *   * \n",
      "\n",
      "    *       * \n",
      "\n",
      "  *           * \n",
      "\n",
      "*               * \n",
      "\n"
     ]
    }
   ],
   "source": [
    "n = int(input(\"Enter the number of rows\"))\n",
    "\n",
    "for i in range(n):\n",
    "    print('  '*(n-1-i) + '* '  , end=\"\")\n",
    "    \n",
    "    if i>=1:\n",
    "        print('  '*(2*i-1) + '* ')\n",
    "    print()"
   ]
